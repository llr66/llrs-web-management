<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.llrjava.mapper.DeptLogMapper">

    <insert id="log">
        insert into dept_log (create_time, description) VALUES
            (#{createTime},#{description})
    </insert>
</mapper>